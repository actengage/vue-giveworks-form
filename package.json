{
  "name": "vue-giveworks-form",
  "version": "0.1.3",
  "description": "A Vue.js component for building and submitting Giveworks form.",
  "main": "dist/giveworks-form.js",
  "module": "dist/giveworks-form.es6.js",
  "author": "Justin Kimbrell",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "lodash-es": "^4.17.4",
    "moment": "^2.20.1",
    "the-one-true-form": "^1.1.3",
    "vue": "^2.5.3",
    "vue-awesome": "^2.3.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-conditional": "^1.1.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-root-import": "^0.2.2",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.5.2",
    "uglify-es": "^3.3.4",
    "vue-template-compiler": "^2.5.13"
  },
  "scripts": {
    "build": "rollup -c --watch; npm run uglify-library;",
    "build-vendor": "rollup -c rollup.vendor.js; npm run uglify-vendor;",
    "uglify": "npm run uglify-library; npm run uglify-vendor;",
    "uglify-vendor": "./node_modules/.bin/uglifyjs dist/vendor.js -o dist/vendor.min.js;",
    "uglify-library": "./node_modules/.bin/uglifyjs dist/giveworks-form.js -o dist/giveworks-form.min.js;"
  }
}
